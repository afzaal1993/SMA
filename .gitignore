# .gitignore for SMA directory

# Ignore node_modules in frontend
/frontend/node_modules
/frontend/dist
/frontend/tmp

# Ignore build output in Backend SMA.API project
/Backend/SMA.API/bin
/Backend/SMA.API/obj

# Ignore logs
/Backend/SMA.API/logs
/Backend/SMA.API/*.log
/frontend/*.log

# Ignore IDE specific files
/.idea
/Backend/.vscode
/Backend/.vs
/frontend/.vscode
*.suo
*.user
*.userosscache
*.sln.docstates

# Ignore npm debug files
npm-debug.log
yarn-debug.log
yarn-error.log

# Ignore coverage folder
/frontend/coverage

# Ignore Angular generated files
/frontend/.angular/cache
